{
    "name": "ShareTo",
    "version": "1.0",
    "description": "Easy sharing!",
    "permissions": [
        "tabs",
        "storage",
        "declarativeContent"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "index.html",
        "default_icon": {
            "72": "images/72.png",
            "96": "images/96.png",
            "128": "images/128.png",
            "144": "images/144.png",
            "152": "images/152.png",
            "192": "images/192.png",
            "384": "images/384.png"
        }
    },
    "icons": {
        "72": "images/72.png",
        "96": "images/96.png",
        "128": "images/128.png",
        "144": "images/144.png",
        "152": "images/152.png",
        "192": "images/192.png",
        "384": "images/384.png"
    },
    "manifest_version": 2
}